#: A_New_Order/units/_main.cfg:4
msgid "race^Akladian"
msgstr ""

#. [race]: id=akladian
#: A_New_Order/units/_main.cfg:5
msgid "race+female^Akladian"
msgstr ""

#. [race]: id=akladian
#: A_New_Order/units/_main.cfg:6
msgid "race^Akladians"
